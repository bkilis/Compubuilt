@model IEnumerable<Compubuilt.Models.DeliveryType>

@{
    ViewData["Title"] = "Delivery methods management";
}
<div class="container">

<h1>Delivery methods management</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DeliveryTypeName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DeliveryTypeName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.DeliveryTypeId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.DeliveryTypeId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.DeliveryTypeId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</div>